          <span class="sr-only">Toggle navigation</span>
          <span class="icon-bar"></span>
          <span class="icon-bar"></span>
          <span class="icon-bar"></span>
        </button>
        <a class="navbar-brand" href="/">Subasta</a>
      </div>
    </div><!-- /.container-fluid -->
    </nav>
    <div class="container">
    <%= yield %>
    </div>
